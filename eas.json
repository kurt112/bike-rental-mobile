{
  "cli": {
    "version": ">= 3.5.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "channel": "staging",
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "android":{
        "serviceAccountKeyPath":  "./assets/serviceAccount.json"
      }
    }
  }
}